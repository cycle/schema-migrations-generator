{
  "name": "cycle/schema-migrations-generator",
  "type": "library",
  "license": "MIT",
  "description": "Cycle ORM Migration generation",
  "require": {
    "php": ">=7.2",
    "cycle/orm": "^1.0",
    "cycle/schema-builder": "^1.0",
    "spiral/migrations": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "cycle/annotated": "^1.0",
    "spiral/debug": "^2.7",
    "spiral/code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Cycle\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Migrations\\Tests\\": "tests/Migrations/"
    }
  }
}
