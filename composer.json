{
  "name": "cycle/schema-migrations-generator",
  "type": "library",
  "license": "MIT",
  "description": "Cycle ORM Migration generation",
  "require": {
    "php": ">=7.2",
    "cycle/database": "^1.0.2",
    "cycle/schema-builder": "^1.2",
    "cycle/migrations": "^2.0"
  },
  "require-dev": {
    "cycle/orm": "^1.7.1",
    "phpunit/phpunit": "~8.0",
    "cycle/annotated": "^2.4",
    "spiral/debug": "^2.7",
    "spiral/code-style": "^1.0"
  },
  "autoload": {
    "files": [
      "src/polyfill.php"
    ],
    "psr-4": {
      "Cycle\\Schema\\Generator\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cycle\\Schema\\Generator\\Migrations\\Tests\\": "tests/Migrations/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
